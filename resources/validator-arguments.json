{
  "model": {
    "type": "string",
    "required": true
  },
  "srs": {
    "type": "string",
    "required": true
  },
  "data-extent": {
    "type": "string",
    "required": false
  },
  "max-errors": {
    "type": "string",
    "required": false,
    "default_value": 50,
    "override_allowed": true
  },
  "normalize": {
    "type": "boolean",
    "required": false,
    "default_value": true,
    "override_allowed": true
  },
  "plugins": {
    "type": "string",
    "required": false
  },
  "encoding": {
    "type": "string",
    "required": false,
    "default_value": "UTF-8",
    "override_allowed": false
  }
}
