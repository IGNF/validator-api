includes:
  - vendor/phpstan/phpstan-phpunit/extension.neon
  - vendor/phpstan/phpstan-phpunit/rules.neon

parameters:
  level: 2
  paths:
    - src
    - tests
  excludes_analyse:
    - tests/bootstrap.php
  bootstrapFiles:
    - bin/.phpunit/phpunit/vendor/autoload.php
  ignoreErrors:
    - '#Call to an undefined method [a-zA-Z0-9\\_]+::findOneBy[A-Z][a-zA-Z]*\(\)#'